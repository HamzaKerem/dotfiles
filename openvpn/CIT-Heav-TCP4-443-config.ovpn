dev tun
persist-tun
persist-key
data-ciphers AES-128-GCM:AES-256-GCM:AES-128-CBC
data-ciphers-fallback AES-128-CBC
auth SHA256
tls-client
client
resolv-retry infinite
remote citlabsheav.tech.purdue.edu 443 tcp4
auth-user-pass
ca CIT-Heav-TCP4-443-ca.crt
tls-auth CIT-Heav-TCP4-443-tls.key 1
remote-cert-tls server

