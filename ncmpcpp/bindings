# Movement stuff
def_key "j"
	scroll_down
def_key "k"
	scroll_up
def_key "l"
	next_column
def_key "h"
	previous_column

def_key "J"
	scroll_down
	scroll_down
	scroll_down
def_key "K"
	scroll_up
	scroll_up
	scroll_up
def_key "ctrl-d"
	scroll_down
	scroll_down
	scroll_down
	scroll_down
	scroll_down
def_key "ctrl-u"
	scroll_up
	scroll_up
	scroll_up
	scroll_up
	scroll_up

# Find stuff
def_key "n"
	next_found_item
