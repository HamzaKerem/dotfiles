# play next 
bind -f common [ player-next
unbind -f common b 
# play previous 
bind -f common ] player-prev
unbind -f common z

# add to que
bind -f common q win-add-q 
# add to playlist
bind -f common p win-add-p

# move track up in playlist
bind -f common J win-mv-after
# move track down in playlist
bind -f common K win-mv-before

# set colorscheme
colorscheme default
